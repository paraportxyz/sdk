{
	"name": "@paraport/sdk",
	"version": "0.1.11",
	"description": "ParaPort Embedded UI",
	"keywords": ["paraport", "polkadot", "sdk"],
	"author": "Paraport",
	"license": "MIT",
	"type": "module",
	"files": ["dist", "README.md"],
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"style": "./dist/index.css",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./style": "./dist/index.css"
	},
	"scripts": {
		"dev": "vite build --watch --mode development",
		"build": "vite build",
		"lint": "biome check --write .",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest run"
	},
	"peerDependencies": {
		"polkadot-api": "1.20.x"
	},
	"dependencies": {
		"@paraport/vue": "workspace:*",
		"@paraport/core": "workspace:*",
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"polkadot-api": "1.20.x",
		"@biomejs/biome": "^1.9.4",
		"@vitest/coverage-v8": "^3.1.2",
		"jsdom": "^26.1.0",
		"typescript": "^5.7.3",
		"vite": "^6.0.7",
		"vite-plugin-dts": "^4.5.0",
		"vitest": "^3.1.2"
	}
}
